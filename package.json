{
  "name": "e-commerce-medicines",
  "version": "1.0.0",
  "description": "WEDO e-commerce prototype front end",
  "main": "index.js",
  "engines": {
    "node": ">=8.16.2",
    "npm": ">=6.4.1"
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint-js": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "lint-css": "stylelint \"src/**/*.{css,less,styl,scss,sass,sss}\"",
    "lint": "yarn run lint-js && yarn run lint-css",
    "fix-js": "yarn run lint-js --fix",
    "fix-css": "yarn run lint-css --fix",
    "fix": "yarn run fix-js && yarn run fix-css",
    "flow": "flow",
    "flow:check": "flow check",
    "dev": "next",
    "build": "next build",
    "start": "next start -p $PORT",
    "heroku-postbuild": "npm run build",
    "test": "jest",
    "test-watch": "yarn run test --watch --notify",
    "test-cover": "yarn run test --coverage",
    "coverage": "yarn run test-cover && opn coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaiogama18/e-commerce-medicines.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kaiogama18/e-commerce-medicines/issues"
  },
  "homepage": "https://github.com/kaiogama18/e-commerce-medicines#readme",
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "formik": "^2.1.4",
    "isomorphic-unfetch": "^3.0.0",
    "material-ui-icons": "^1.0.0-beta.36",
    "next": "^9.3.4",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-owl-carousel2": "^0.3.0",
    "react-router-dom": "^5.1.2",
    "react-window": "^1.8.5",
    "sass": "^1.26.3",
    "stylelint-config-recommended": "^3.0.0",
    "tailwindcss": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-flow": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "browser-sync": "^2.26.7",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "flow-bin": "^0.122.0",
    "jest": "^25.3.0",
    "pure-react-carousel": "^1.27.0",
    "react-responsive-carousel": "^3.2.4",
    "react-socks": "^2.1.0",
    "stylelint": "^13.3.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0"
  }
}
